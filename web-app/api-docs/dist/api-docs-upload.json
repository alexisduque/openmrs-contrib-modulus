{
  "apiVersion": "0.0.1",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:8080/api",
  "resourcePath": "/upload",
  "produces": [
    "application/json"
  ],

  "apis": [
    {
      "path": "/upload/release/{releaseID}",
      "operations": [
        {
          "method": "PUT",
          "summary": "Upload a file to a pre-existing release",
          "notes": "Content-Length header must be provided so the server can verify it has received the entire upload. When submitting through a modern browser (like you are now), Content-Length will be auto-filled",
          "type": "Release",
          "nickname": "show",
          "consumes": [
            "application/octet-stream"
          ],
          "parameters": [
            {
              "name": "releaseID",
              "description": "numeric ID of a release",
              "paramType": "path",
              "type": "string",
              "required": "true"
            },
            {
              "name": "filename",
              "description": "filename (including extension) of the binary data being uploaded",
              "paramType": "query",
              "type": "string",
              "required": "true"
            },
            {
              "name": "rawFile",
              "description": "the entire binary content of the file being uploaded",
              "paramType": "body",
              "type": "file",
              "required": "true"
            }
          ],
          "responseMessages": [
            {
              "code": 400,
              "message": "Invalid ID supplied"
            },
            {
              "code": 411,
              "message": "Content-Length header not provided"
            },
            {
              "code": 404,
              "message": "Release not found"
            }
          ]

        }
      ]
    }
  ],


  "models": {
    "Release": {
      "id": "Release",
      "required": ["moduleVersion", "requiredOMRSVersion", "releasedBy"],
      "properties": {
        "moduleVersion": {
          "type": "string"
        },
        "requiredOMRSVersion": {
          "type": "string",
          "description": "minimum version of OpenMRS compatible"
        },
        "releasedBy": {
          "type": "User"
        },
        "contentType": {
          "type": "string",
          "description": "auto-generated"
        },
        "filename": {
          "type": "string",
          "description": "filename of the omod uploaded"
        },
        "dateCreated": {
          "type": "date"
        },
        "lastUpdated": {
          "type": "date"
        }
      }
    }
  }
}